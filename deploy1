apiVersion: apps/v1
kind: Deployment
metadata:
  name: first-hello-os
  labels:
    app: hello
spec:
  replicas: 1
  selector:
    matchLabels:
      app: hello
  template:
    metadata:
      labels:
        app: hello
    spec:
      containers:
      
      - image: 244740641779.dkr.ecr.us-east-1.amazonaws.com/aws_depolyment_team2:latest
        name: hello
 
        ports:
        - containerPort: 8080

        resources:
          requests:
            memory: "128Mi"
            cpu: "500m"
          limits:
            memory: "128Mi"
            cpu: "500m"
       
